public static int getFirstSetBitPos(int n){
        
   if(n==1) return 1;
        int count=0;
        while(n>0)
        {
            if(( (n) & 1) ==0)
            {
                count++;
            }
            else
                { count++ ; return count;
                 }

            n=n>>1;
        }
        return count;
}
