  public static boolean checkPanagram  (String ssd)
    {
           boolean a1[]=new boolean[256];
      
        String s=ssd.toLowerCase();

        for(int i=0;i<s.length();i++)
        {
             a1[s.charAt(i)]=true;
        }

        for(int i=97;i<=122;i++)
        {
            if(a1[i]==false){return false;}
        }

        return true;
    }
