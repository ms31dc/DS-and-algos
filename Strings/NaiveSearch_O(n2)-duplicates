static boolean search(String pat, String txt)
    {
           
int i;
int j;
int m=txt.length();
int n=pat.length();
        for( i=0;i<=m-n;i++)
        {
             for(  j=0;j<n;j++)
             {
                 if(txt.charAt(i+j)!=pat.charAt(j))
                 {break;}

             }
                if(j==n){return true;}


        }


        return false;
    }
    
