 public boolean isIdentical (Node head1, Node head2){
        //write your code here and return true if they are identical, otherwise false
        
        Node n1=head1;
        Node n2=head2;
        
        while(n1!=null && n2!=null)
        {
            if(n1.data!=n2.data){return false;}
            
            n1=n1.next;
            n2=n2.next;
            
        }
        
        return true;
        
    }
