public static void merge(int arr1[], int arr2[], int m, int n)
    {
        // add your code here


        for(int i=n-1;i>=0;i--)
        {
            int last=arr1[m-1];
            int j;
            for(  j=m-2;(m>1) && (arr1[j]>arr2[i])    ;j--)
            {
                arr1[j+1]=arr1[j];
            }

            if(j!=m-2 || (last>arr2[i]))
            {
                arr1[j+1]=arr2[i];
                arr2[i]=last;
            }

        }
    }
