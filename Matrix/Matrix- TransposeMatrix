class TransposeMatrix
{
    // Function to find the traspose of matrix
    // a[][]: input matrix
    // n: given size of matrix
    static void transpose(int a[][], int n)
    {
       
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                int temp = a[i][j];
               a[i][j] = a[j][i];
                a[j][i] = temp;


            }
        }
    }
}
