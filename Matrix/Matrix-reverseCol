static void reverseCol(int n1, int m1, int arr1[][])
    {
        for(int i=0;i<n1;i++)
        {
            int low=0, high=m1-1;

            while(low<high)
            {
                int temp=arr1[i][low];
                arr1[i][low]=arr1[i][high];
                arr1[i][high]=temp;
                low++;high--;
            }

        }
    }
